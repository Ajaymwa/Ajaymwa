local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib(" Ajay | SprinX (Winx) [Magical Fairy Dust] (MINI GUI)", "Synapse")

local Tab = Window:NewTab("Normal selection")
local PlayerSection = Tab:NewSection("Choose a normal player to play as")

PlayerSection:NewButton("Bloom", "ButtonInfo", function()
    print("Clicked")
    -- Script generated by Nut The Menace Spy - credits to Claudev0#8849

local args = {
    [1] = "BloomNormal"
}

game:GetService("ReplicatedStorage").ChangePlayerCharacter:FireServer(unpack(args))

end)

