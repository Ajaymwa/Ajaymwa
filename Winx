local Tab = Window:NewTab("Player Mod")
local ModSection = Tab:NewSection("Admin, walkspeed, jumppower, ect... ")

ModSection:NewButton("Infinite yield", "ButtonInfo", function()
    print("Clicked")
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

ModSection:NewSlider("Walkspeed", "SliderInfo", 500, 14, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

